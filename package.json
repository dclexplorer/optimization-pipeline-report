{
  "name": "optimization-pipeline-report",
  "version": "2.0.0",
  "private": true,
  "description": "Asset optimization pipeline report generator for Decentraland",
  "workspaces": [
    "ui"
  ],
  "scripts": {
    "build": "tsc",
    "build:ui": "npm run build --workspace=ui",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:ui": "npm run dev --workspace=ui",
    "generate": "npm run build && npm start"
  },
  "keywords": ["decentraland", "optimization", "report"],
  "author": "DCL Explorer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dclexplorer/optimization-pipeline-report.git"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@vercel/postgres": "^0.5.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}