{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "public",
  "framework": null,
  "installCommand": "npm install",
  "build": {
    "env": {
      "S3_ACCESS_KEY_ID": "@s3-access-key-id",
      "S3_SECRET_ACCESS_KEY": "@s3-secret-access-key",
      "S3_ENDPOINT": "@s3-endpoint",
      "S3_BUCKET": "@s3-bucket",
      "S3_REGION": "@s3-region"
    }
  },
  "functions": {
    "api/status.js": {
      "maxDuration": 10
    }
  }
}